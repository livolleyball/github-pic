{
    "manifest_version": 3,
    "name": "GithubImagePlugin",
    "version": "1.0",
    "description": "Click on images to see them in a floating view.",
    "icons": {
      "48": "github.png"
    },
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "content_scripts": [
      {
        "matches": ["https://github.com/*"],
        "css": ["styles.css"],
        "js": ["popup.js"]
      }
    ]
  }
  